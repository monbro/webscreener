{
  "manifest_version": 2,

  "name": "Webscreener [EXPERIMENTAL]",
  "description": "Control every website remote via any device.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://secure.flickr.com/",
    "http://localhost/*",
    "http://localhost/sockjs/*",
    "tabs", "https://vimeo.com/*"
  ],
  "web_accessible_resources": [
      "css/style.css",
      "css/bootstrap-combined.min.css",
      "lib/jquery.min.map"
  ],
  "background": {
      "scripts": [
          "lib/jquery.min.js",
          "background.js"
          ]
  },
  "content_scripts": [  {
      "matches": [
          "https://vimeo.com/*",
          "http://vimeo.com/*"
          ],
      "css": [
          "css/bootstrap-combined.min.css",
          "css/style.css"
          ],
      "js": [
          "lib/jquery.min.js",
          "lib/sockjs-0.3.min.js",
          "lib/hashids.js",
          "lib/uuid.js",
          "lib/jquery.min.map",
          "lib/doccookies.js",
          "lib/meteor-ddp.js",
          "inject.js"
          ]
    }
  ]
}