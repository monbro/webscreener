{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "This extension demonstrates a 'browser action' with kittens.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://secure.flickr.com/",
    "http://localhost/*",
    "tabs", "http://webregard.de/*"
  ],
  "background": {
      "scripts": [
          "jquery.min.js",
          "background.js"
          ]
  },
  "content_scripts": [  {
      "matches": [
          "http://www.webregard.de/*",
          "https://vimeo.com/*"
          ],
      "js": [
          "jquery.min.js",
          "sockjs-0.3.min.js",
          "inject.js"
          ]
    }
  ]
}